<Service
    UrlProvider="QSMetadataUrlProvider">

    <Urls>
        
        <Url Name="host">http://api.xiachufang.com/v2/issues/</Url>

    </Urls>
    
    <Defaults
    Method="GET"
    Message="努力加载中..."
    Timeout="20"
    TimeoutMessage="数据加载失败,请检查当前网络状况..."
    FallbackMessage="数据加载失败,请检查当前网络状况..."/>
    
    <ServiceMethods>
        
        <ServiceMethod
        Method="GET"
        Timeout="20"
        UrlDomain="list.json"
        Url="host"
        ReturnType="HomeInfo"/>

    </ServiceMethods>
    
</Service>